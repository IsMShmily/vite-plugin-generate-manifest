    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-node-resolve": "^15.3.1",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/ali-oss": "^6.16.11",
        "@types/archiver": "^6.0.3",
        "@types/node": "^20.0.0",
        "@types/node-fetch": "^2.6.12",
        "rollup": "^4.40.2",
        "rollup-plugin-dts": "^6.2.1",
        "typescript": "^5.0.0",
        "vite": "^4.0.0"
